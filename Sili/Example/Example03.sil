type("Find the sum of 55, 67, 124, 572, 126, 121")
function sum(a, b)
{
    finalSum = 0
    for(i = 0; i < b; i++i)
    {
        finalSum += a[i]
    }
    return finalSum
}
arr = [55, 67, 124, 572, 126, 121]
x = sum(arr, 6)
type(x)
type("Find the average of the sum")
x /= 6
